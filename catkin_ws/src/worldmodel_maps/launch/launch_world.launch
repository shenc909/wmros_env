<?xml version="1.0"?>
<launch>
    <arg name="world_name" default="Bowtie_track" />
    <arg name="gui" default="false"/>
 
  <include file="$(find gazebo_ros)/launch/empty_world.launch">
    <arg name="world_name" value="$(find worldmodel_maps)/worlds/$(arg world_name).world"/>
    <arg name="gui" value="$(arg gui)"/>
    <arg name="verbose" value="true" />
  </include>

</launch>
